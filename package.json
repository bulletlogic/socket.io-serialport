{
  "name": "socket.io-serialport",
  "version": "1.0.0",
  "description": "Expose a serial port over a socket.io instance",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "iocat": "iocat --socketio ws://localhost:8080",
    "iocatPort": "iocat --socketio ws://localhost:8080/serialport",
    "wscat": "wscat -c 'ws://localhost:8080/socket.io/?EIO=3&transport=websocket'",
    "wscatPort": "wscat -c 'ws://localhost:8080/socket.io/serialport/?EIO=3&transport=websocket'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pghalliday/socket.io-serialport.git"
  },
  "keywords": [
    "websocket",
    "socket.io",
    "serialport"
  ],
  "author": "Peter Halliday <pghalliday@gmail.com> (http://pghalliday.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pghalliday/socket.io-serialport/issues"
  },
  "homepage": "https://github.com/pghalliday/socket.io-serialport#readme",
  "dependencies": {
    "mkdirp": "^0.5.1",
    "serialport": "^4.0.3",
    "socket.io-stream": "^0.9.1"
  },
  "devDependencies": {
    "iocat": "^0.4.1",
    "socket.io": "^1.5.0",
    "wscat": "^1.0.1"
  }
}
